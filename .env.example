# CoinGecko API
VITE_COINGECKO_API_KEY=

# Optional: override API root if you use a proxy or different endpoint
VITE_COINGECKO_API_ROOT=

# Set to "true" to use pro-api endpoint (requires a pro key and may bill you)
VITE_COINGECKO_USE_PRO_API=false

# Auto-refresh interval in milliseconds. Set to 0 to disable polling.
# Recommended: 30000 (30s) - 60000 (60s) depending on rate limits.
VITE_AUTO_REFRESH_MS=30000
